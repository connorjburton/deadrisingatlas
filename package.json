{
  "private": true,
  "name": "dr2combos",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build",
    "serve": "sirv build --cors --single",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.2.3",
    "eslint": "^7.14.0",
    "eslint-config-preact": "^1.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-preset-preact": "^4.0.2",
    "preact-cli": "^3.0.3",
    "preact-render-spy": "^1.2.1",
    "sirv-cli": "^1.0.8"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash.orderby": "^4.6.0",
    "preact": "^10.5.7",
    "preact-render-to-string": "^5.1.12",
    "preact-router": "^3.2.1"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/tests/test_unit/**/*.js"
    ]
  }
}
