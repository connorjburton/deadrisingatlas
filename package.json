{
  "name": "dr2combos",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build",
    "serve": "sirv build --cors --single",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^3.4.0",
    "eslint": "^8.11.0",
    "eslint-config-preact": "^1.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-preset-preact": "^4.0.2",
    "preact-cli": "^3.5.1",
    "preact-render-spy": "^1.2.1",
    "sirv-cli": "^2.0.2",
    "classnames": "^2.2.6",
    "lodash.orderby": "^4.6.0",
    "preact": "^10.6.6",
    "preact-render-to-string": "^5.1.20"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/tests/*.js"
    ]
  }
}
